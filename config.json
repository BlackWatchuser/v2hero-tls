{
  "inbounds": [
  {
    "listen": "127.0.0.1",
    "port": 42348,
    "protocol": "vmess",
    "tag": "proxy",
    "settings": {
      "clients": [
        {"id": "$UUID1", "email": "justsoso", "level": 0, "alterId": 64},
        {"id": "$UUID2", "email": "justsoso1", "level": 0, "alterId": 64}
      ]
    },
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
        "path": "/"
      }
    }
  }
  ],

  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  }],

  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  }
}